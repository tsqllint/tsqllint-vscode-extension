{
	"name": "tsqllint",
	"description": "A VS Code Extension For TSQLLint",
	"author": "Nathan Boyd",
	"license": "MIT",
	"version": "0.0.3",
	"publisher": "tsqllint",
	"main": "./client/out/src/extension",
	"icon": "images/icon.png",
	"activationEvents": [
		"onLanguage:sql"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tsqllint/tsqllint-vscode-extension"
	},
	"engines": {
		"vscode": "^1.15.0"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^6.0.88",
		"tslint": "^5.9.1",
		"typescript": "^2.5.2",
		"vsce": "^1.35.0"
	},
	"scripts": {
		"lint": "tslint -c ./tslint.json '**/src/**/*.ts'",
		"install": "cd server && npm install && cd ../client && npm install"
	}
}
